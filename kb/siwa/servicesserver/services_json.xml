<sd:varParameter>img=@_replace/(.*?)FULLTEXT(.*?)tekst_(.*?).xml/_inurl_by$1JPEG$2$3.jpg@&amp;</sd:varParameter>
<sd:varParameter>words=@_query@&amp;</sd:varParameter>
<sd:varParameter>format=SGD&amp;scale=2.08</sd:varParameter>
</sd:varParameters>
